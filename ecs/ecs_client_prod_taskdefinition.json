{
  "containerDefinitions": [
    {
      "name": "client",
      "image": "%s.dkr.ecr.us-east-1.amazonaws.com/my-dev-space-client:production",
      "essential": true,
      "memoryReservation": 300,
      "portMappings": [
        {
          "hostPort": 0,
          "protocol": "tcp",
          "containerPort": 3000
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "my-dev-space-client-prod",
          "awslogs-region": "us-east-1"
        }
      }
    }
  ],
  "family": "my-dev-space-client-prod-td"
}
